// List of actions
.container
  div.loading2(ng-hide="loaded")
    div.bullets
    div.bullet
    div.bullet
    div.bullet
    div.bullet
  div(ng-cloak="" ng-show="loaded")
    .list-unstyled.action-list#list-of-actions
      ul.list-unstyled.action-list#list-of-actions
        li(ng-repeat="action in actions | orderBy:sortOrder | filter:filterActions | filter:searchText as results track by $index")
          .action-container
            .action-image-container(style="cursor: pointer" ng-click="actionDetailOpen('sm', action)")
              h1.repeatedNumber(ng-show="action.repeatedNumber"){{action.repeatedNumber}}
              img.placeholder-image(src="../img/image-placeholder.png" ng-show="!action.image || action.image === 'data:image/png;base64,'")
              img(ng-src="{{action.image}}" ng-hide="!action.image || action.image === ''")
            hr
            ul.list-inline.action-list-info
              li
                ul.list-inline
                  li.col.col-xs-9
                    .action-name.line-clamp {{action.actionName}}
                    .by-username(ng-hide="!action.user.firstName && !action.user.lastName") {{action.user.firstName}} {{action.user.lastName}}
                    .by-username(ng-show="!action.user.firstName && !action.user.lastName") {{action.user.username}}
                    
                  li.col.col-xs-3.pull-right
                    img#timerOK(src="../../img/ok-icon.png", ng-show="action.completed === false" ng-click="scheduleCompleteActionModal(action)")
                    img#timerOK(src="../../img/timer-icon.png", ng-show="action.completed === true || action.completed === undefined" ng-click="scheduleCompleteActionModal(action)")
                hr
    
              ul.list-inline.action-list-info2(style="cursor: pointer" ng-click="actionDetailOpen('sm', action)")
                li#date-text
                  p {{action.startedDatetime | date: 'MMM d y'}} - {{action.startedDatetime | date: 'HH:mm:ss'}}
                li
                  img(src="img/clock-icon.png")
                  {{action.timeSpent}} min
                li
                  img(src="img/money-icon.png")
                  {{action.moneySpent | currency}}
                  
                li
                  img(src="img/location-icon.png")
                  {{action.location}}
                li
                  img(src="img/repeat-mini-icon.png")
                  {{action.repeat | number}}
          div.double(ng-class="{'isVisible' : !{{action.repeatedNumber || false}}}")
          div.triple(ng-class="{'isVisible' : !{{action.repeatedNumber || false}}}")
          br
          